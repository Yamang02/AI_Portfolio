# Gemini AI API 설정
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# GitHub 설정
VITE_GITHUB_USERNAME=your_github_username

# 애플리케이션 설정
VITE_APP_NAME=AI Portfolio Chatbot
VITE_APP_DESCRIPTION=개발자 포트폴리오 AI 챗봇
VITE_CONTACT_EMAIL=contact@example.com
VITE_APP_VERSION=1.0.0
VITE_DEVELOPER_NAME=이정준
VITE_DEVELOPER_TITLE=시니어 프론트엔드 엔지니어 & AI 애호가

# EmailJS 설정 (선택사항 - 설정하지 않으면 mailto로 폴백)
# EmailJS.com에서 가입 후 서비스, 템플릿, API 키를 생성하여 설정
VITE_EMAILJS_SERVICE_ID=service_xxxxxxx
VITE_EMAILJS_TEMPLATE_ID=template_xxxxxxx
VITE_EMAILJS_PUBLIC_KEY=xxxxxxxxxxxxxxxxxxxx

# 배포 설정
VITE_PROJECT_ID=your-project-id
VITE_REGION=asia-northeast3
VITE_SERVICE_NAME=ai-portfolio-chatbot

# 개발 환경 설정
VITE_API_BASE_URL=http://localhost:3000
VITE_ENVIRONMENT=development

# 프로덕션 환경 설정 (Cloud Run)
# VITE_API_BASE_URL=https://your-app-url.run.app
# VITE_ENVIRONMENT=production  